# Audio hardware
ro.hardware.audio.primary=r8q

# Display
ro.surface_flinger.set_display_power_timer_ms=2000
ro.surface_flinger.set_idle_timer_ms=400
ro.surface_flinger.set_touch_timer_ms=8000

# Fingerprint
ro.vendor.fingerprint.type=udfps_optical
ro.vendor.fingerprint.sensor_location=540|2144|100
ro.vendor.fingerprint.rectangular_sensor_location=421,2025,659,2263
ro.vendor.fingerprint.request_touch_event=true

# Sensors
ro.vendor.sensors.samsung.udfps=true
